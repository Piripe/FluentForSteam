                7="image(x0,y1-4,x0+4,y1,fluent/controls/textEntry/page/normal/bl.tga)"
                8="image_tiled(x0,y0+4,x0+4,y1-4,fluent/controls/textEntry/page/normal/l.tga)"
            }
        }
        TextEntry:empty {
            render_bg {
                0="fill( x0+4, y0+4, x1-8, y1-4, textEntryNormal)"
