"button.styles" {
    include "fluent/colors.styles"

	styles {

        TextEntry {
			minimum-height=32
            font-family=basefont
            font-size=18
            font-weight=400
            textcolor="white"
            bgcolor="none"
            selectedtextcolor="white"
            selectedbgcolor="textEntrySelectionBackground"
            shadowtextcolor="text"  // this is the cursor color

            inset-left=4
            inset-top=0
			padding-left="10"
			padding-bottom="10"
			padding-top="10"
			padding-right="10"
            render_bg {
                0="fill( x0+4, y0+4, x1-4, y1-4, textEntryNormal)"
                1="image(x0,y0,x0+4,y0+4,fluent/controls/textEntry/normal/tl.tga)"
                2="image_tiled(x0+4,y0,x1-4,y0+4,fluent/controls/textEntry/normal/t.tga)"
                3="image(x1-4,y0,x1,y0+4,fluent/controls/textEntry/normal/tr.tga)"
                4="image_tiled(x1-4,y0+4,x1,y1-4,fluent/controls/textEntry/normal/r.tga)"
                5="image(x1-4,y1-4,x1,y1,fluent/controls/textEntry/normal/br.tga)"
                6="image_tiled(x0+4,y1-4,x1-4,y1,fluent/controls/textEntry/normal/b.tga)"
                7="image(x0,y1-4,x0+4,y1,fluent/controls/textEntry/normal/bl.tga)"
                8="image_tiled(x0,y0+4,x0+4,y1-4,fluent/controls/textEntry/normal/l.tga)"
            }
        }

        "Page TextEntry" {
