                2="image_tiled(x0+4,y0,x1-4,y0+4,fluent/controls/textEntry/normal/t.tga)"
                3="image(x1-4,y0,x1,y0+4,fluent/controls/textEntry/normal/tr.tga)"
                4="image_tiled(x1-4,y0+4,x1,y1-4,fluent/controls/textEntry/normal/r.tga)"
                5="image(x1-4,y1-4,x1,y1,fluent/controls/textEntry/normal/br.tga)"
