            }
        }

        "Page TextEntry:focus" {
            textcolor="white"
            render_bg {
                0="fill( x0+4, y0+4, x1-4, y1-4, textEntryPageFocus)"
                1="image(x0,y0,x0+4,y0+4,fluent/controls/textEntry/page/focus/tl.tga)"
                2="image_tiled(x0+4,y0,x1-4,y0+4,fluent/controls/textEntry/page/focus/t.tga)"
                3="image(x1-4,y0,x1,y0+4,fluent/controls/textEntry/page/focus/tr.tga)"
                4="image_tiled(x1-4,y0+4,x1,y1-4,fluent/controls/textEntry/page/focus/r.tga)"
                5="image(x1-4,y1-4,x1,y1,fluent/controls/textEntry/page/focus/br.tga)"
